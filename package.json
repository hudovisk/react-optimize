    "@babel/register": "^7.4.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "all-contributors-cli": "^6.11.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
